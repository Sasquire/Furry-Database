{
  "dependencies": {
    "async": "^3.2.4",
    "axios": "^1.5.0",
    "binary-search": "^1.3.6",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.3.1",
    "log4js": "^6.9.1",
    "nanoid": "^4.0.2",
    "pako": "^2.1.0",
    "pg": "^8.11.3",
    "readable-stream-clone": "^0.0.7"
  },
  "ecmaVersion": "2022",
  "sourceType": "module",
  "type": "module",
  "eslintConfig": {
    "extends": "standard",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off",
      "indent": [
        "error",
        "tab",
        {
          "SwitchCase": 1
        }
      ],
      "no-tabs": "off",
      "camelcase": "off",
      "generator-star-spacing": "off",
      "yield-star-spacing": "off"
    }
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0"
  }
}
